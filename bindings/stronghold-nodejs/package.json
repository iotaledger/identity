{
  "name": "@iota/identity-stronghold-nodejs",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "identity-stronghold-nodejs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "ava": "^5.1.1",
    "rollup": "^3.21.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.0",
    "txm": "^8.1.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@iota/identity-wasm": "0.7.0-alpha.6"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "npm run build:napi && npm run build:ts",
    "build:ts": "rollup -c --failAfterWarnings",
    "build:napi": "napi build --platform --release --js napi.js --dts napi.d.ts napi-dist",
    "build:debug": "napi build --platform",
    "create-npm-dir": "napi create-npm-dir --target ./ ",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@3.5.0"
}
